/* pages/moneyoutlist/moneyoutlist.wxss */

.item_view{
  background:#FFFFFF;
  text-align: left;
  width: 100%;
  height:80px;
}

.item_line{
  background:#F2F3F7;
  text-align: left;
  width: 100%;
  height:10px;
}

.item_location_view{
  flex-direction: row;
  display: flex;
  justify-content: space-between;
  text-align: center;
  height: 30px;
}


.item_time{
  font: 12px arial;
  position: relative;
  align-items: right;
  margin-top: 4px;
  margin-right: 10px;
  color: #999999;
}

.item_detail{
  font: 12px arial;
  color: #999999;
  margin-left: 10px;
  margin-right: 10px;
}

.item_tip{
  font: 12px arial;
  color: #999999;
  margin-left: 10px;
  line-height: 20px;
}

.item_tip_detail{
  font: 12px arial;
  color: #999999;
  margin-right: 10px;
  line-height: 20px;
}

.item_tip_distance{
  font: 12px arial;
  color: #999999;
  margin-top: 3px;
}

.img{
  width: 20px;
  height: 20px;
  margin-top: 2px;
  margin-left: 5px;
  margin-right: 5px;
}



